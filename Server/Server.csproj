		  <IncludeAassets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAassets>
	  </PackageReference>
	  <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="7.0.3" />
	  <PackageReference Include="Swashbuckle.AspNetCore.Filter" Version="7.0.12" />
